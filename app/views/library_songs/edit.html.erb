<h1>Editing library_song</h1>

<% form_for(@library_song) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :song_id %><br />
    <%= f.text_field :song_id %>
  </p>
  <p>
    <%= f.label :library_id %><br />
    <%= f.text_field :library_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @library_song %> |
<%= link_to 'Back', library_songs_path %>